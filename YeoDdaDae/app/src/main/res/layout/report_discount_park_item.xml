<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="100dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/reportDiscountParkNameTxt"
        android:text="주차장 이름"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp" />

    <TextView
        android:id="@+id/reportDiscountParkConditionAndDiscountTxt"
        android:text="조건 / 혜택"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/reportDiscountParkNameTxt"
        app:layout_constraintLeft_toLeftOf="@+id/reportDiscountParkNameTxt"/>

    <TextView
        android:id="@+id/reportDiscountParkRateTxt"
        android:text="0 / 0 / 0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/reportDiscountParkConditionAndDiscountTxt"
        app:layout_constraintLeft_toLeftOf="@+id/reportDiscountParkConditionAndDiscountTxt"/>

    <TextView
        android:id="@+id/reportDiscountParkIDTxt"
        android:text="ID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/reportDiscountParkNameTxt"
        android:layout_marginBottom="10dp"/>

    <TextView
        android:id="@+id/reportDiscountParkUpTimeTxt"
        android:text="시각"
        android:textSize="15sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="10dp"
        android:layout_marginRight="10dp" />

</androidx.constraintlayout.widget.ConstraintLayout>